@model IEnumerable<Dominio.Entidades.Reserve>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.dataEntrada)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.horarioEntrada)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dataSaida)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.horarioSaida)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.custoDia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.custoTotal)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Estacionamento)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.dataEntrada)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.horarioEntrada)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.dataSaida)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.horarioSaida)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.custoDia)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.custoTotal)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Estacionamento.cidade)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
